{
  "name": "cannect-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:e2e": "start-server-and-test dev http://localhost:3000 cy:open-e2e",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:open-e2e": "cypress open --e2e --browser electron",
    "cy:open-unit": "cypress open --component --browser electron"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "sass": "^1.67.0",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.1",
    "@cypress/react18": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@swc/plugin-styled-components": "^1.5.86",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.27",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.34.4",
    "cypress": "^13.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "lightningcss": "^1.22.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "prettier": "^3.0.3",
    "sort-by": "^1.2.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^0.34.4"
  },
  "nyc": {
    "all": true,
    "report-dir": "cypress/coverage"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}